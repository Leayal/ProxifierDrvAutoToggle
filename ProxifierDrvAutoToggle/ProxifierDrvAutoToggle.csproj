<Project Sdk="Microsoft.NET.Sdk.Worker">

  <PropertyGroup>
    <TargetFramework>net9.0-windows7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <InvariantGlobalization>true</InvariantGlobalization>
    <PublishAot>true</PublishAot>
	<PublishWmiLightStaticallyLinked>true</PublishWmiLightStaticallyLinked>
	<CsWin32RunAsBuildTask>true</CsWin32RunAsBuildTask>
	<DisableRuntimeMarshalling>true</DisableRuntimeMarshalling>
    <UserSecretsId>dotnet-ProxifierDrvAutoToggle-0ef7d7a6-8c67-46e2-aa43-3170d7d7fd16</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Hosting" Version="10.0.0" />
	<PackageReference Include="Microsoft.Extensions.Hosting.WindowsServices" Version="10.0.0" />
    <PackageReference Include="Microsoft.Windows.CsWin32" Version="0.3.248">
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="WmiLight" Version="6.16.0" />
  </ItemGroup>
</Project>
